#container {
    background-color: #f0f4f3;
}
.question-box {
    padding: 40rpx 20rpx 10rpx;
}

.tag {
    background-color: #eef5f8;
    padding: 15rpx 20rpx;
    border-radius: 28rpx;
    margin-right: 20rpx;
    color: #2186e0;
}

.question-name {
    padding: 40rpx 20rpx 30rpx;
    font-size: 38rpx;
}

.dec {
    padding: 0 20rpx;
    font-size: 26rpx;
}

.icons {
    width: 50rpx;
    height: 50rpx;
}

.text {
    color: #afafaf;
    font-size: 24rpx;
}

.eeee {
    display: flex;
    align-items: center;
}

.lf {
    display: flex;
}

.mes {
    display: flex;
    padding: 10rpx;
    justify-content: space-between;
    margin: 20rpx;
}
#container .bt {
    width: 160rpx;
    height: 76rpx;
    line-height: 76rpx;
    font-size: 26rpx;
    font-weight: 400;
    margin: 0;
}

.sel {
    border-top: 1rpx solid #ebebeb;
    border-bottom: 1rpx solid #ebebeb;
    height: 94rpx;
    font-size: 26rpx;
    color: #889091;
}

.btn-l {
    border-right: 4rpx solid #ebebeb;
    height: 100%;
}

.bg {
    background-color: #fff;
}

.list {
    margin-top: 7rpx;
    padding: 30rpx 30rpx 20rpx;
    background-color: #fff;
}

.text {
    font-size: 26rpx;
    color: #a0acac;
    margin-left: 14rpx;
}

.header {
    display: flex;
    align-items: center;
}

.main {
    display: block;
    font-size: 24rpx;
    color: #5b5b5b;
    margin-top: 10rpx;
}
.bodys {
    margin-top: 10rpx;
}
.cda {
    margin-top: 10rpx;
    font-size: 24rpx;
    color: #a0acac;
}
